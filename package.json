{
  "name": "@oopsunome/fast-gitignore",
  "description": "添加 / 更新 `.gitignore`。内嵌 `@oopsunome/fast-gitignore` 维护但来自 `github/gitignore` 的模板。",
  "bin": {
    "fast-gitignore": "esm/main.js"
  },
  "main": "esm/fastGitignore.js",
  "scripts": {
    "start": "DEBUG=GITIGNORE:* npx babel-node ./dev",
    "cli": "yarn build > /dev/null && ./esm/main.js -o .",
    "build": "npx babel-node ./scripts/clean-built && npx babel-node ./scripts/build",
    "clean": "npx babel-node ./scripts/clean",
    "license": "npx babel-node ./scripts/license",
    "reprepare": "npx babel-node ./scripts/re-prepare",
    "lint": "npx eslint --cache ./src/**/*.js",
    "changelog": "npx auto-changelog -p",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "files": [
    "src/**/*.js",
    "esm/**/*.js",
    "templates/**/*.gitignore"
  ],
  "dependencies": {
    "callsites": "^3.1.0",
    "chalk": "^2.4.2",
    "cosmiconfig": "^5.2.1",
    "fast-glob": "^3.0.3",
    "lodash": "^4.17.11",
    "meow": "^5.0.0",
    "p-map": "^2.1.0",
    "redent": "^3.0.0",
    "update-notifier": "^3.0.1",
    "write-file-atomic": "^3.0.0"
  },
  "private": false,
  "gitignore": [
    "macOS",
    "Windows",
    "Linux",
    "Node",
    "VisualStudioCode",
    "SublimeText",
    "CVS",
    "Diff",
    "Vim",
    "TortoiseGit"
  ],
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "auto-changelog": "^1.13.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-add-module-exports": "^1.0.2",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "execa": "^2.0.3",
    "license-checker": "^25.0.1",
    "listr": "^0.14.3",
    "make-dir": "^3.0.0",
    "snyk": "^1.192.0",
    "trash": "^6.0.0"
  },
  "keywords": [
    "git",
    "gitignore",
    "初始化",
    "init",
    "脚手架",
    "scaffold",
    "生成器",
    "generator",
    "工具",
    "tool",
    "cli",
    "oopsunome"
  ],
  "version": "1.0.8",
  "author": {
    "name": "iyowei",
    "email": "swzyocowboy@icloud.com",
    "url": "https://github.com/iTonyYo"
  },
  "repository": {
    "url": "git@github.com:iTonyYo/fast-gitignore.git",
    "type": "git"
  },
  "homepage": "https://github.com/iTonyYo/fast-gitignore",
  "bugs": "https://github.com/iTonyYo/fast-gitignore/issues",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "eslintIgnore": [
    "*-test.js",
    "esm/**",
    ".github/**",
    ".vscode/**",
    "scripts/**",
    "coverage/**"
  ],
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "globals": {
      "require": true,
      "process": true,
      "module": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parser": "babel-eslint",
    "rules": {
      "no-console": 0
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10.13.0"
          }
        }
      ]
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "changelog-template.hbs",
    "unreleased": true,
    "commitLimit": false,
    "includeBranch": [
      "master",
      "develop"
    ]
  },
  "snyk": true
}
